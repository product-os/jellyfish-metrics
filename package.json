{
  "name": "@balena/jellyfish-metrics",
  "version": "1.0.345",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-metrics.git"
  },
  "engines": {
    "node": ">=12.15.0"
  },
  "description": "Metrics library for Jellyfish",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib && npm run check && deplint && depcheck --ignore-bin-package --ignores=@types/jest",
    "lint:fix": "balena-lint --fix lib",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "check": "jellycheck"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-environment": "^5.0.5",
    "@balena/jellyfish-logger": "^3.0.86",
    "@balena/node-metrics-gatherer": "^5.7.5",
    "express": "^4.17.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.3",
    "@balena/jellyfish-config": "^1.5.4",
    "@balena/jellyfish-types": "^1.2.3",
    "@balena/lint": "^6.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^14.17.12",
    "@types/uuid": "^8.3.1",
    "depcheck": "^1.4.2",
    "deplint": "^1.1.3",
    "jest": "^27.1.0",
    "lint-staged": "^11.1.2",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.6.1",
    "ts-jest": "^27.0.5",
    "typedoc": "^0.21.8",
    "typescript": "^4.4.2",
    "uuid": "^8.3.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2021-10-18T23:17:08.621Z"
  }
}
