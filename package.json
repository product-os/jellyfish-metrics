{
  "name": "@balena/jellyfish-metrics",
  "version": "1.0.424",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-metrics.git"
  },
  "engines": {
    "node": ">=12.15.0"
  },
  "description": "Metrics library for Jellyfish",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib && deplint && depcheck --ignore-bin-package --ignores=@types/jest,typedoc",
    "lint:fix": "balena-lint --fix lib",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "Apache-2.0",
  "dependencies": {
    "@balena/jellyfish-environment": "^5.2.0",
    "@balena/jellyfish-logger": "^3.0.156",
    "@balena/node-metrics-gatherer": "^5.7.5",
    "express": "^4.17.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@balena/jellyfish-config": "^2.0.1",
    "@balena/jellyfish-types": "^1.2.3",
    "@balena/lint": "^6.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@types/uuid": "^8.3.3",
    "depcheck": "^1.4.2",
    "deplint": "^1.1.3",
    "jest": "^27.4.3",
    "lint-staged": "^12.1.2",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.7.0",
    "ts-jest": "^27.0.7",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.2",
    "uuid": "^8.3.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2021-12-01T10:29:18.038Z"
  }
}
